{
  "name": "adedejistephen.com-v2",
  "version": "1.0.0",
  "private": true,
  "description": "adedejistephen.com-v2",
  "author": "ionware",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|yml|yaml|md|css|scss)\"",
    "lint": "eslint --fix \"src/**/*.+(js|jsx)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
