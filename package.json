  },
  "lint-staged": {
    "src/**/*.+(js|jsx|json|yml|yaml|md)": [
      "prettier --write"
    ],
    "src/**/*.+(js|jsx)": [
      "eslint --fix --quiet"
    ]
  },
  "dependencies": {
    "gatsby": "^3.11.0",
    "gatsby-plugin-postcss": "^4.11.0",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7"
  }
